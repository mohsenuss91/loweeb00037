<script type="text/javascript">
$('a[href^="#"]').ready(function(){
    $('a[href^="#decouvrir"]').on('click', function(evt){
        evt.preventDefault(); 
        var target = $(this).attr('href');
        $('html, body')
            .stop()
            .animate({scrollTop: $(target).offset().top}, 1000 ); 
    });
});
</script>
